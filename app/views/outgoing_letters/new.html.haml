%h2=l(:label_new_outgoing_letter)

- labelled_form_for @object, :url => outgoing_letters_path, :html => {:id => 'object-form', :multipart => true} do |f|
  = render :partial => 'form', :locals => { :f => f }
  = f.hidden_field :organization_id  
  = submit_tag l(:button_create)
  = submit_tag l(:button_create_and_continue), :name => 'continue'
  -# link_to_remote l(:label_preview),
    { :url => preview_outgoing_letter_path,
    :method => 'get',
    :update => 'preview',
    :with => "Form.serialize('object-form')"
    }, :accesskey => accesskey(:preview)
